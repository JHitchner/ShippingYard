<h1> Active Jobs </h1>
<% @jobs.each do |job| %>
  <tr>
    <td><%= job.jobTitle %></td><br>
    <td><%= job.description %></td><br>
    <td><%= link_to 'Show', job_path(job) %></td>
  </tr>
  <%= link_to 'Edit', edit_job_path(job) %>
  <%= link_to 'Destroy', job_path(job),
              method: :delete,
              data: { confirm: 'Are you sure?' } %>
<% end %>
